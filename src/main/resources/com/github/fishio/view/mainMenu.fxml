<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="480.0" minWidth="640.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.github.fishio.control.MainMenuController">
  <children>
    <Canvas fx:id="canvas" height="480.0" width="640.0" />
    <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="423.0" prefWidth="640.0">
      <children>
        <ImageView fitHeight="150.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="logo.png" />
          </image>
          <VBox.margin>
            <Insets top="20.0" />
          </VBox.margin>
        </ImageView>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
          <children>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
              <children>
                <Button fx:id="btnSingleplayer" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#startSinglePlayer" prefWidth="120.0" text="Singleplayer" />
                <Button fx:id="btnShowHighscores" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#showHighScores" prefWidth="120.0" text="High scores" />
                <Button fx:id="btnShowHelp" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#showHelp" prefWidth="120.0" text="Help" />
              </children>
              <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
              </HBox.margin>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.margin="$x1">
              <children>
                <Button fx:id="btnMultiplayer" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#startMultiplayer" prefWidth="120.0" text="Multiplayer" />
                <Button fx:id="btnShowAchievements" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#showAchievements" prefWidth="120.0" text="Achievements" />
                <Button fx:id="btnShowSettings" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#showSettings" prefWidth="120.0" text="Settings" />
              </children>
            </VBox>
          </children>
        </HBox>
        <Button fx:id="btnExit" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#exitGame" prefWidth="120.0" text="Exit">
          <VBox.margin>
            <Insets bottom="10.0" />
          </VBox.margin>
        </Button>
      </children>
    </VBox>
  </children>
</StackPane>
